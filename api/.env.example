APP_ENV=development
APP_NAME=Billetera API
APP_VERSION=0.1.0
LOG_LEVEL=INFO

SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
# Option A (recommended for local IPv4): Supavisor pooler (Transaction mode)
# Format: postgresql://postgres.<project-ref>:<db_password>@aws-<n>-<region>.pooler.supabase.com:6543/postgres
# Option B (direct): postgresql://postgres:<db_password>@db.<project-ref>.supabase.co:5432/postgres
SUPABASE_DB_URL=postgresql://postgres.your-project-ref:YOUR_DB_PASSWORD@aws-1-your-region.pooler.supabase.com:6543/postgres
SUPABASE_JWT_ISSUER=
SUPABASE_JWT_AUDIENCE=authenticated
